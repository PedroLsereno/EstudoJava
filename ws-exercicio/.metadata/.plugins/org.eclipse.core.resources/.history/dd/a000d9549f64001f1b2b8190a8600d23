
public class Vetor {
	private int valor;

	public Vetor(int valor) {
		this.valor = valor;
	}

	public int getValor() {
		return valor;
	}

	public void setValor(int valor) {
		this.valor = valor;
	}
	
	public int valoresNegativos(int N) {
		System.out.println("Valores Negativos: ");
		for(int i = 0; i < N; i++) {
			if (valor < 0) {
				System.out.println(valor);
			}
		}
		return valoresNegativos();
	}
}
