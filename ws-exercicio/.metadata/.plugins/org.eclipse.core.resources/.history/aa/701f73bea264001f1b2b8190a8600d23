import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Quantos números vão ser adicionados?");
		int N = sc.nextInt();
		
		Main[] vet = new Main[N];
		
		int valor;
		for (int i = 0; i < vet.length; i++ ) {
			valor = sc.nextInt();
			vet[i] = valor;
		}
		
		
		
		
		sc.close();
	}

}
