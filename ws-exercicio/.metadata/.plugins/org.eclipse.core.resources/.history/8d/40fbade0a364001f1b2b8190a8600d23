import java.util.Scanner;

public class Vetor {
	private int[] vet;

	public Vetor(int N) {
		vet = new int[N];
	}

	public int[] getVet() {
		return vet;
	}

	public void setVet(int[] vet) {
		this.vet = vet;
	}
	
	public void addValor() {
		int valor;
		for (int i = 0; i < vet.length; i++ ) {
			valor = sc.nextInt();
			vet[i] = valor;
		}
	}
	
	
}
