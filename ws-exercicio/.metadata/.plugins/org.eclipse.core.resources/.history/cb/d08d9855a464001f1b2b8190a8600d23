import java.util.Scanner;

public class Vetor {
	private int[] vet;

	public Vetor(int N) {
		vet = new int[N];
	}

	public int[] getVet() {
		return vet;
	}

	public void setVet(int valor) {
		this.vet = valor;
	}
	
	
	public void addValor() {
		int valor;
		for (int i = 0; i < vet.length; i++ ) {
			vet[i].setVet(valor);
		}
	}
	
	
	
}
