public class Vetor {
	private int valor;

	public Vetor(int valor) {
		super();
		this.valor = valor;
	}

	public int getValor() {
		return valor;
	}

	public void setValor(int valor) {
		this.valor = valor;
	}

	public String valoresNegativos(int n) {
		System.out.println("Valores Negativos: ");
		for(int i = 0; i < n; i++) {
			if (vetor[i] < 0) {
				System.out.println(vetor[i]);
			}
		}
		return valoresNegativos(n);
	}

}
