public class Vetor {
	private int[] vetor;


	
	public Vetor(int[] vetor) {
		this.vetor = vetor;
	}
	
	
	public int[] getVetor() {
		return vetor;
	}


	public void setVetor(int[] vetor) {
		this.vetor = vetor;
	}

	
	public String valoresNegativos() {
		System.out.println("Valores Negativos: ");
		for(int i = 0; i < vetor.length; i++) {
			if (vetor[i] < 0) {
				System.out.println(vetor[i]);
			}
		}
		return valoresNegativos();
	}
}
